<mvc:View
 height="100%"
	controllerName="sap.ui.demo.worklist.controller.Ranking"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:semantic="sap.f.semantic"
	  xmlns:tnt="sap.tnt"
	displayBlock="true">


	<tnt:ToolPage id="toolPage2">
		<tnt:header>
			<tnt:ToolHeader>
				<Button id="sideNavigationToggleButton2"
						icon="sap-icon://menu2"
						type="Transparent"
						press="onSideNavButtonPress">
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow" />
					</layoutData>
				</Button>
				

			</tnt:ToolHeader>
		</tnt:header>
		<tnt:sideContent>
			<tnt:SideNavigation expanded="true"
								itemSelect="onItemSelect">
				<tnt:NavigationList items="{path: 'menu>/navigation'}">
					<tnt:NavigationListItem text="{menu>title}"
											icon="{menu>icon}"
											enabled="{menu>enabled}"
											expanded="{menu>expanded}"
											items="{menu>items}"
											key="{menu>key}">
						<tnt:NavigationListItem text="{title}"
												key="{key}"
												enabled="{enabled}">
						</tnt:NavigationListItem>
					</tnt:NavigationListItem>
				</tnt:NavigationList>
				<tnt:fixedItem>
					<tnt:NavigationList items="{path: '/fixedNavigation'}">
						<tnt:NavigationListItem text="{title}"
												icon="{icon}">
						</tnt:NavigationListItem>
					</tnt:NavigationList>
				</tnt:fixedItem>
			</tnt:SideNavigation>
		</tnt:sideContent>
		<tnt:mainContents>
			<NavContainer id="pageContainer2">
				<pages>
					<ScrollContainer id="root2"
									 horizontal="false"
									 vertical="true"
									 height="100%">
						<Text id="rankingText" text="{
				path: 'backEnd>/rankingText'				
			}" />
			
					</ScrollContainer>
					
					
				</pages>
			</NavContainer>
			
		</tnt:mainContents>
	</tnt:ToolPage>
</mvc:View>
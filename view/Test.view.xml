<mvc:View
   xmlns="sap.m"
   xmlns:mvc="sap.ui.core.mvc"
   xmlns:table="sap.ui.table"
   controllerName="sap.ui.demo.whoseturn.controller.Test">
   <Text text="Hello World"/>
   		<table:Table  id="FilesTree"
		    	      				  width="auto"
					                  rows="{path:'frontEnd>/FileList', parameters: {arrayNames:['logEntries']}}"
					                  selectionMode="None"
					                  enableSelectAll="false"
        							  expandFirstLevel="false"
			            			  rowSelectionChange="onFileSelect">
						<table:layoutData>
							<l:GridData span="L12 M12 S12" />
						</table:layoutData>
<!-- 		                <table:toolbar> -->
<!-- 		                    <OverflowToolbar> -->
<!-- 				                <ToolbarSpacer/> -->
<!-- 								<Button text="Gernerate PDF Collective" press="onPDFCollective" type="Emphasized" visible="{frontEnd>/ToolbarVisible/FileAction}"  /> -->
<!-- 								<Button text="Send PDF via Mail" press="onPDFMailCollective" type="Emphasized" visible="{frontEnd>/ToolbarVisible/FileAction}"  /> -->
<!-- 								<Button text="Send to Printer" press="onPrintCollective" type="Emphasized" visible="{frontEnd>/ToolbarVisible/FileAction}" /> -->
<!-- 								<Button text="Order Entry" press="onOrderEntryCollective" type="Emphasized" visible="{frontEnd>/ToolbarVisible/FileAction}" /> -->
<!-- 								<Button text="Download" press="onDownload" type="Emphasized" visible="{frontEnd>/ToolbarVisible/DownloadAction}" /> -->
<!-- 								<Button text="Retry" press="onRetry" type="Emphasized" visible="{frontEnd>/ToolbarVisible/RetryAction}" /> -->
<!-- 		                    </OverflowToolbar> -->
<!-- 		                </table:toolbar> -->
		                <table:columns>
		                    <table:Column width="8rem">
		                        <Label text="{i18n>AWB}"/>
		                        <table:template>
			                    	<Text text="{frontEnd>AWB}"/>
		                        </table:template>
		                    </table:Column>
		                    <table:Column width="5rem">
		                        <Label text="{i18n>FOID}"/>
		                        <table:template>
			                    	<Text text="{frontEnd>FOID}"/>
		                        </table:template>
		                    </table:Column>
		                    <table:Column width="6rem">
		                        <Label text="{i18n>CreateDate}"/>
		                        <table:template>
		                            <Text text="{ path: 'frontEnd>Crea',
												    type: 'sap.ui.model.type.DateTime',
													       formatOptions: {
													 		style: 'short'
											          	   }
												  }"/>
		                        </table:template>
		                    </table:Column>
		                    <table:Column width="6rem">
		                        <Label text="{i18n>OrderPartyKey}"/>
		                        <table:template>
		                            <Text text="{frontEnd>OrderParty}"/>
		                        </table:template>
		                    </table:Column>
		                    <table:Column width="9rem">
		                        <Label text="{i18n>OrderPartyDesc}"/>
		                        <table:template>
		                            <Text text="{frontEnd>OrderPartyDesc}"/>
		                        </table:template>
		                    </table:Column>
		                    <table:Column width="3rem">
		                        <Label text="{i18n>Print}"/>
		                        <table:template>
		                            <Button text="" press="onPrint" type="Emphasized" icon="sap-icon://print" />
		                        </table:template>
		                    </table:Column>
		                </table:columns>
		            </table:Table>
</mvc:View>
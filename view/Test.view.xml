<mvc:View
   xmlns="sap.m"
   xmlns:mvc="sap.ui.core.mvc"
   controllerName="sap.ui.demo.whoseturn.controller.Test">
   <Text text="Hello World"/>
   		<Table
				id="table"
				busyIndicatorDelay="{worklistView>/tableBusyDelay}"
				growing="true"
				growingScrollToLoad="true"
				noDataText="{worklistView>/tableNoDataText}"
				updateFinished="onUpdateFinished"
				width="auto"
				rows="{path:'frontEnd>/Users'}">				
					<columns>
						<Column id="nameColumn">
							<Label
								text="{i18n>TableNameColumnTitle}"/>
								<table:template>
		                            <Text text="{frontEnd>name}"/>
		                        </table:template>
						</Column>
						<Column
							id="supplierNameColumn"
							demandPopin="false"
							minScreenWidth="Tablet">
							<Label text="{i18n>TableSupplierColumnTitle}"/>
							<table:template>
		                            <CheckBox text="{frontEnd>isTraveling}"/>
		                        </table:template>
						</Column>
						<Column
							id="unitPriceColumn"
							hAlign="End"
							demandPopin="true"
							minScreenWidth="Tablet">
							<Label text="{i18n>TablePriceColumnTitle}"/>
							<table:template>
		                            <CheckBox text="{frontEnd>isDriving}"/>
		                        </table:template>
						</Column>
				</columns>

			</Table>
</mvc:View>
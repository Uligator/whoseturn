<mvc:View
   xmlns="sap.m"
   xmlns:mvc="sap.ui.core.mvc"
   controllerName="sap.ui.demo.whoseturn.controller.Test">
   <Text text="Hello World"/>
   		<Table id="idProductsTable"
		inset="false"
		items="{
			path: 'frontEnd>/Users'
		}">
			<items>
			<ColumnListItem>
				<cells>
					<ObjectIdentifier
						title="{Name}"
						text="{name}"/>
					<CheckBox
						selected="{isTraveling}" />
					<CheckBox
						selected="{isDriving}" />
				</cells>
			</ColumnListItem>
		</items>
	</Table>
</mvc:View>